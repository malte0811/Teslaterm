{
  "include": ["js/**/*.ts", "js/**/*.tsx"],
  "compilerOptions": {
    "typeRoots": [
      "node_modules/@types",
      "js/types"
    ],
    "downlevelIteration": true,
    "module": "commonjs",
    "target": "es5",
    "sourceMap": true,
    "inlineSources": true,
    "lib": [
      "es5",
      "es2015",
      "dom",
      "scripthost"
    ],
    "esModuleInterop": true,
    "jsx": "react-jsx"
  }
}
